class Solution:
    def kidsWithCandies(self, candies, extraCandies):


        ansar=[]

        m=max(candies)

        for i in candies:
            if i+extraCandies>=m:
                ansar.append(True)
            else:
                ansar.append(False)

        return ansar

s=Solution()

print(s.kidsWithCandies([2,3,5,1,3],3))