class Solution:
    def isPalindrome(self, s):

      ar="".join(s.split()).lower()
      newar="".join([ch for ch in ar if ch.isalnum()])
      print(newar)
      rear=newar[::-1]
      print(rear)
      if newar==rear:
        return True
      else:
        return False

s=Solution()
print(s.isPalindrome("0p"))
