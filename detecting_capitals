class Solution(object):
    def detectCapitalUse(self, word):
        ''' To check if a word has capital use or not'''

        if word.isupper():
          return True
        elif word.istitle():
          return True
        elif word.islower():
          return True
        else:
          return False


s=Solution()
print(s.detectCapitalUse("WhatK"))
